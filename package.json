{
  "name": "mm-image-cropper",
  "version": "0.0.1",
  "description": "image cropper",
  "scripts": {
    "build-standalone": "npm run lint && webpack --progress --colors && gulp docs",
    "lint": "jshint src",
    "watch": "watch \"npm run build\" src",
    "copy-resources": "rsync -av --exclude=\"*.js\" src/* out/",
    "build": "npm run lint && npm run copy-resources && babel src --out-dir out --source-maps inline --optional runtime,spec.protoToAssign",
    "clean": "rm -rf out docs",
    "prepublish": "npm run build",
    "test": "npm run build-standalone && karma start",
    "jenkins-test": "npm run build-standalone && karma start jenkins.karma.conf.js",
    "build-docs": "npm run build-standalone && gulp docs",
    "live-docs": "npm run build-standalone && gulp live-docs"
  },
  "main": "out/index.js",
  "license": "Apache-2.0",
  "dependencies": {
    "angular": "^1.4.3",
    "babel-runtime": "^5.8.20"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.3",
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.15.6",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "glob": "^5.0.14",
    "gulp": "^3.9.0",
    "gulp-ngdocs": "^0.2.13",
    "gulp-util": "^3.0.6",
    "html-loader": "^0.3.0",
    "http-server": "^0.8.0",
    "jasmine-core": "^2.3.4",
    "jshint": "^2.8.0",
    "karma": "^0.13.8",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.6",
    "karma-jasmine-html-reporter": "^0.1.8",
    "karma-junit-reporter": "^0.3.3",
    "karma-nested-reporter": "^0.1.3",
    "karma-ng-html2js-preprocessor": "^0.1.2",
    "karma-phantomjs-launcher": "^0.2.1",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "less-plugin-npm-import": "^2.1.0",
    "lodash": "^3.10.1",
    "magnet-test-utils": "^1.0.0",
    "phantomjs": "^1.9.18",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.12.3",
    "watch": "^0.16.0",
    "webpack": "^1.11.0"
  }
}
